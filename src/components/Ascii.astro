import React, { useEffect, useState } from "react";
import getAsciiImage from "get-ascii-image";

const asciiArt = async function generateAscii(imageUrl: string) {
  console.log("jnkv", imageUrl);
  try {
    const art = await getAsciiImage(imageUrl, {
      maxWidth: 200,
      maxHeight: 200,
    });

    console.log(art);
  } catch (error) {
    console.error("Error generating ASCII art:", error);
  }
};
